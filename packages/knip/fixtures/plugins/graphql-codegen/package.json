{
  "name": "@fixtures/graphql-codegen",
  "version": "*",
  "scripts": {
    "codegen": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "*"
  },
  "codegen": {
    "schema": "schema.graphql",
    "documents": [
      "src/**/*.tsx",
      "!src/gql/**/*"
    ],
    "generates": {
      "./src/gql/": {
        "preset": "client"
      }
    }
  }
}
