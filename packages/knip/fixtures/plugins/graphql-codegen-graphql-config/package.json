{
  "name": "@fixtures/graphql-codegen-graphql-config",
  "scripts": {
    "codegen": "graphql-codegen"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "*",
    "graphql-config": "*"
  },
  "graphql": {
    "schema": "schema.graphql",
    "documents": ["src/**/*.tsx", "!src/gql/**/*"],
    "extensions": {
      "codegen": {
        "generates": {
          "./src/gql/": {
            "preset": "client"
          }
        }
      }
    }
  }
}
