{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "include": ["classMembers"],
  "workspaces": {
    ".": {
      "project": ["!templates/**"]
    },
    "packages/knip": {
      "project": ["src/**/*.ts!", "!src/util/empty.ts", "!**/_template/**"]
    },
    "packages/docs": {
      "entry": ["{remark,scripts}/*.ts"]
    }
  }
}
