{
  "name": "knip",
  "version": "0.13.1",
  "description": "Find unused files, dependencies and exports in your TypeScript and JavaScript project",
  "keywords": [
    "find",
    "detect",
    "unused",
    "unreferenced",
    "files",
    "dependencies",
    "exports",
    "types",
    "duplicates",
    "typescript",
    "maintenance",
    "unimported"
  ],
  "repository": "github:webpro/knip",
  "homepage": "https://github.com/webpro/knip",
  "bugs": "https://github.com/webpro/knip/issues",
  "main": "dist/index.js",
  "bin": {
    "knip": "dist/cli.js"
  },
  "type": "module",
  "scripts": {
    "knip": "node ./dist/cli.js",
    "test": "globstar -- node --loader tsx --test \"test/*.spec.ts\"",
    "watch": "tsc --watch",
    "build": "rm -rf dist && tsc",
    "prepublishOnly": "npm test && npm run build && npm run knip",
    "format": "remark README.md -o",
    "release": "release-it"
  },
  "files": [
    "dist"
  ],
  "author": {
    "name": "Lars Kappert",
    "email": "lars@webpro.nl"
  },
  "license": "ISC",
  "dependencies": {
    "@snyk/github-codeowners": "1.0.0",
    "easy-table": "1.2.0",
    "esbuild": "0.15.13",
    "esbuild-register": "3.3.3",
    "globby": "13.1.2",
    "micromatch": "4.0.5",
    "summary": "2.1.0",
    "ts-morph": "16.0.0",
    "ts-morph-helpers": "0.6.1"
  },
  "devDependencies": {
    "@types/micromatch": "4.0.2",
    "@types/node": "18.11.5",
    "globstar": "1.0.0",
    "prettier": "2.7.1",
    "release-it": "15.5.0",
    "remark-cli": "11.0.0",
    "remark-preset-webpro": "0.0.1",
    "tsx": "3.11.0",
    "type-fest": "3.1.0",
    "typescript": "4.8.4"
  },
  "release-it": {
    "github": {
      "release": true
    }
  },
  "remarkConfig": {
    "plugins": [
      "preset-webpro"
    ]
  },
  "engines": {
    "node": ">=16.17.0 <17 || >=18.6.0"
  },
  "engineStrict": true
}
